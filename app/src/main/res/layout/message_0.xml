<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/Cardwhite"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbartest"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_marginBottom="8dp"
        android:background="#075e54"
        android:elevation="4dp">



        <ImageView
            android:id="@+id/imageView"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:padding="8dp"
            android:src="@drawable/previous"
            android:layout_marginStart="16sp"

            />

        <TextView
            android:layout_width="wrap_content"
            android:id="@+id/Test"
            android:layout_height="wrap_content"
            android:text="Lecture Schedule"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:layout_marginLeft="15dp"/>

    </androidx.appcompat.widget.Toolbar>

    <androidx.cardview.widget.CardView
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        app:cardCornerRadius="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-RCS sir"
                android:textSize="14sp"
                android:textColor="#B3A9A9"
                android:layout_weight="1"
                android:layout_marginLeft="6dp"
                android:layout_marginTop="3dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=" +91 1234567890"
                android:textSize="12sp"
                android:textColor="#B3A9A9"
                android:layout_margin="5dp"
                android:layout_marginLeft="6dp"
                android:layout_gravity="center" />
        </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="English Grammer"
                android:layout_marginTop="3dp"
                android:textColor="#3AA7D8"
                android:textStyle="bold"
                android:layout_marginLeft="7dp"
                android:gravity="start"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:text="Teacher:-XYX"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Date:-DD/MM/YYYY"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Time:-12.45PM-04.00PM"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Address:- Block No: 7/9/10,First Floor, Omkar Building, Beside Potoba Restaurant, MIDC, Dombivli East"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="7dp"
                android:textColor="@color/black"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="12.45PM"
                android:gravity="end"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="7dp"
                android:layout_marginRight="10dp"
                android:textColor="#B3A9A9"
                />

        </LinearLayout>

        <!--        <ImageView-->
        <!--            android:layout_width="22dp"-->
        <!--            android:layout_height="22dp"-->
        <!--            android:padding="2dp"-->
        <!--            android:layout_gravity="right"-->
        <!--            android:layout_marginRight="7dp"-->
        <!--            android:layout_marginTop="148dp"-->
        <!--            android:src="@drawable/dpubl"/>-->


